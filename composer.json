{
    "name": "scriptoshi/livewire-siwe",
    "description": "Sign in with Ethereum integration for Laravel using Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Scriptoshi",
            "email": "scriptoshi@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "livewire/livewire": "^3.0",
        "kornrunner/keccak": "^1.0",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Scriptoshi\\LivewireSiwe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scriptoshi\\LivewireSiwe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scriptoshi\\LivewireSiwe\\LivewireSiweServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}